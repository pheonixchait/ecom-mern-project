       </Menu.Item>\r\n\r\n            <Menu.Item key=\"shop\" icon={<ShoppingOutlined />}>\r\n                <Link to=\"/shop\">Shop</Link>\r\n            </Menu.Item>\r\n\r\n            <Menu.Item key=\"cart\" icon={<ShoppingCartOutlined />}>\r\n                <Link to=\"/cart\">\r\n                    <Badge count={cart.length} offset={[9, 0]}>\r\n                        Cart\r\n                    </Badge>\r\n                </Link>\r\n            </Menu.Item>\r\n\r\n            {!user && (\r\n                <>\r\n                    <Menu.Item key=\"register\" icon={<UserAddOutlined />} className=\"float-right\">\r\n\r\n                        <Link to=\"/register\">Register</Link>\r\n                    </Menu.Item>\r\n\r\n                    <Menu.Item key=\"login\" icon={<UserOutlined />} className=\"float-right\">\r\n\r\n                        <Link to=\"/login\">Login</Link>\r\n                    </Menu.Item>\r\n                </>\r\n            )}\r\n\r\n\r\n            {user && (\r\n                <SubMenu\r\n                    key=\"SubMenu\"\r\n                    icon={<SettingOutlined />}\r\n                    title={user.email && user.email.split('@')[0]}\r\n                    className=\"float-right\"\r\n                >\r\n\r\n                    {user && user.role === \"subscriber\" && (\r\n                        <Menu.Item>\r\n                            <Link to=\"/user/history\">Dashboard</Link>\r\n                        </Menu.Item>\r\n                    )}\r\n\r\n                    {user && user.role === \"admin\" && (\r\n                        <Menu.Item>\r\n                            <Link to=\"/admin/dashboard\">Dashboard</Link>\r\n                        </Menu.Item>\r\n                    )}\r\n\r\n                    <Menu.Item key=\"logout\" icon={<LogoutOutlined />} onClick={logout}>Logout</Menu.Item>\r\n                </SubMenu>\r\n            )}\r\n            <span className=\"float-right p-1\">\r\n                <Search />\r\n            </span>\r\n        </Menu>\r\n    )\r\n}\r\n\r\nexport default Header;","D:\\ecommerce learning project\\client\\src\\components\\routes\\LoadingToRedirect.js",[],"D:\\ecommerce learning project\\client\\src\\components\\category\\CategoryList.js",[],"D:\\ecommerce learning project\\client\\src\\components\\forms\\Search.js",[],["370","371"],"D:\\ecommerce learning project\\client\\src\\components\\Cards\\SingleProduct.js",["372","373","374","375","376","377","378"],"D:\\ecommerce learning project\\client\\src\\index.js",[],["379","380"],"D:\\ecommerce learning project\\client\\src\\pages\\admin\\category\\CategoryUpdate.js",["381","382"],"import React, { useState, useEffect } from 'react'\r\nimport AdminNav from '../../../components/nav/AdminNav'\r\nimport { useSelector } from 'react-redux'\r\nimport { toast } from 'react-toastify'\r\nimport { useParams } from 'react-router-dom'\r\n\r\nimport { getCategory, updateCategory } from '../../../functions/category'\r\n\r\nimport CategoryForm from '../../../components/forms/CategoryForm'\r\n\r\nconst CategoryUpdate = ({ history, match }) => {\r\n\r\n    const [name, setName] = useState(\"\")\r\n    const [loading, setLoading] = useState(false)\r\n    const { user } = useSelector((state) => ({ ...state }));\r\n\r\n    useEffect(() => {\r\n        loadCategory()\r\n    }, [])\r\n\r\n    const loadCategory = () => {\r\n        getCategory(match.params.slug).then((c) => setName(c.data.category.name)) //calling get category is totaly unneccessary, based on slug we are gettimg category name\r\n    }\r\n    const handleSubmit = (e) => {\r\n        e.preventDefault()\r\n        //console.log(name)\r\n        setLoading(true)\r\n        updateCategory(match.params.slug, { name }, user.token)\r\n            .then(res => {\r\n                console.log(res)\r\n                setLoading(false)\r\n                setName('')\r\n                toast.success(`\"${res.data.name}\" is updated`)\r\n                history.push(\"/admin/category\")\r\n            })\r\n            .catch((err) => {\r\n                console.log(err)\r\n                setLoading(false)\r\n                if (err.response.status === 400) toast.error(err.response.data)\r\n            })\r\n    }\r\n\r\n    return (\r\n        <div className=\"container-fluid\">\r\n            <div className=\"row\">\r\n                <div className=\"col-md-2\"><AdminNav /></div>\r\n                <div className=\"col\">\r\n                    {loading ? <h4 className=\"text-danger\">Loading...</h4> : <h4>Update category</h4>}\r\n                    <CategoryForm handleSubmit={handleSubmit} name={name} setName={setName} />\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default CategoryUpdate","D:\\ecommerce learning project\\client\\src\\pages\\Cart.js",[],"D:\\ecommerce learning project\\client\\src\\components\\Cards\\ProductCardInCheckout.js",["383","384","385"],"import React from 'react'\r\nimport ModalImage from 'react-modal-image'\r\nimport laptop from '../../images/laptop.jpg'\r\nimport { useDispatch } from 'react-redux'\r\nimport { toast } from 'react-toastify'\r\nimport { CheckCircleOutlined, CloseCircleOutlined, CloseOutlined } from '@ant-design/icons'\r\n\r\nconst ProductCardInCheckout = ({ p }) => {\r\n    const colors = [\"Black\", \"Brown\", \"Silver\", \"White\", \"Blue\"];\r\n    const dispatch = useDispatch();\r\n\r\n    const handleColorChange = (e) => {\r\n        let cart = [];\r\n        if (typeof window !== \"undefined\") {\r\n            if (localStorage.getItem(\"cart\")) {\r\n                cart = JSON.parse(localStorage.getItem(\"cart\"));\r\n            }\r\n\r\n            cart.map((product, i) => {\r\n                if (product._id === p._id) {\r\n                    cart[i].color = e.target.value;\r\n                }\r\n            })\r\n\r\n            //console.log(\"cart\", cart)\r\n            localStorage.setItem(\"cart\", JSON.stringify(cart));\r\n            dispatch({\r\n                type: \"ADD_TO_CART\",\r\n                payload: cart,\r\n            })\r\n        }\r\n    }\r\n\r\n    const handleQuantityChange = (e) => {\r\n        let cart = [];\r\n        let count = e.target.value < 1 ? 1 : e.target.value;\r\n        if (count > p.quantity) {\r\n            toast.error(`Max available quantity: ${p.quantity}`)\r\n            return;\r\n        }\r\n        //console.log(\"quant \", p.quantity)\r\n        if (typeof window !== \"undefined\") {\r\n            if (localStorage.getItem(\"cart\")) {\r\n                cart = JSON.parse(localStorage.getItem(\"cart\"));\r\n            }\r\n\r\n            cart.map((product, i) => {\r\n                if (product._id === p._id) {\r\n                    cart[i].count = count;\r\n                }\r\n            })\r\n\r\n            //console.log(\"cart\", cart)\r\n            localStorage.setItem(\"cart\", JSON.stringify(cart));\r\n            dispatch({\r\n                type: \"ADD_TO_CART\",\r\n                payload: cart,\r\n            })\r\n        }\r\n    }\r\n\r\n    const handleRemove = () => {\r\n        let cart = [];\r\n        if (typeof window !== \"undefined\") {\r\n            if (localStorage.getItem(\"cart\")) {\r\n                cart = JSON.parse(localStorage.getItem(\"cart\"));\r\n            }\r\n\r\n            cart.map((product, i) => {\r\n                if (product._id === p._id) {\r\n                    cart.splice(i, 1);\r\n                }\r\n            })\r\n\r\n            //console.log(\"cart\", cart)\r\n            localStorage.setItem(\"cart\", JSON.stringify(cart));\r\n            dispatch({\r\n                type: \"ADD_TO_CART\",\r\n                payload: cart,\r\n            })\r\n        }\r\n    }\r\n\r\n    return (\r\n        <tbody>\r\n            <tr>\r\n                <td>\r\n                    <div style={{ width: \"100px\", height: \"auto\" }}>\r\n                        {p.images.length ?\r\n                            (<ModalImage small={p.images[0].url} large={p.images[0].url} />) :\r\n                            (<ModalImage small={laptop} large={laptop} />)}\r\n                    </div>\r\n                </td>\r\n                <td>{p.title}</td>\r\n                <td>${p.price}</td>\r\n                <td>{p.brand}</td>\r\n                <td>\r\n                    <select onChange={handleColorChange} name=\"color\" className=\"form-control\">\r\n                        {p.color ? <option key={p.color} value={p.color}>{p.color}</option> : <option>Select</option>}\r\n                        {colors\r\n                            .filter((c) => c !== p.color)\r\n                            .map((c) =>\r\n                                <option key={c} value={c}>{c}</option>)}\r\n                    </select>\r\n                </td>\r\n                <td className=\"text-center\">\r\n                    <input type=\"number\" className=\"form-control\" value={p.count} onChange={handleQuantityChange}></input>\r\n                </td>\r\n                <td className=\"text-center\">\r\n                    {\r\n                        p.shipping === \"Yes\" ?\r\n                            <CheckCircleOutlined className=\"text-success\" /> :\r\n                            <CloseCircleOutlined className=\"text-danger\" />\r\n                    }\r\n                </td>\r\n                <td className=\"text-center\">\r\n                    <CloseOutlined className=\"text-danger pointer\" onClick={handleRemove} />\r\n                </td>\r\n            </tr>\r\n        </tbody>\r\n    )\r\n}\r\n\r\nexport default ProductCardInCheckout;","D:\\ecommerce learning project\\client\\src\\reducers\\userReducer.js",[],"D:\\ecommerce learning project\\client\\src\\pages\\user\\Wishlist.js",["386","387"],"D:\\ecommerce learning project\\client\\src\\reducers\\drawerReducer.js",[],"D:\\ecommerce learning project\\client\\src\\com